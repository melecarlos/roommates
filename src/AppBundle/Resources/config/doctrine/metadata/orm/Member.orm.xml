<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Member" table="member">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="forename" type="string" column="forename" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastname" type="string" column="lastname" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="string" column="type" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ip" type="string" column="ip" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="createdDate" type="datetime" column="created_date" nullable="true"/>
  </entity>
</doctrine-mapping>
