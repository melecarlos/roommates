<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Header" table="header">
    <indexes>
      <index name="fk_header_member1_idx" columns="member"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="serie" type="string" column="serie" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="days" type="string" column="days" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="startDateSerie" type="date" column="start_date_serie" nullable="true"/>
    <field name="endDateSerie" type="date" column="end_date_serie" nullable="true"/>
    <field name="ip" type="string" column="ip" length="15" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="createdDate" type="datetime" column="created_date" nullable="true"/>
    <many-to-one field="member" target-entity="Member" fetch="LAZY">
      <join-columns>
        <join-column name="member" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
