<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Quota" table="quota">
    <indexes>
      <index name="fk_quota_member1_idx" columns="member"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="maxPersons" type="integer" column="max_persons" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="minPersons" type="integer" column="min_persons" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="status" type="string" column="status" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="createdDate" type="datetime" column="created_date" nullable="true"/>
    <many-to-one field="member" target-entity="Member" fetch="LAZY">
      <join-columns>
        <join-column name="member" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
